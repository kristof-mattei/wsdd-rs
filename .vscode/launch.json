    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'wsdd-rs'",
            "cargo": {
                "args": ["build", "--bin=wsdd-rs", "--package=wsdd-rs"],
                "filter": {
                    "name": "wsdd-rs",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "DEBUG,wsdd_rs=TRACE"
            },
            "internalConsoleOptions": "neverOpen",
            "terminal": "integrated"
        },
